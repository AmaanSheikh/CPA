        <StackPanel Height="64" Margin="70,20,82,10" Orientation="Horizontal">
            <Button x:Name="button_Copy" FontFamily="Segoe MDL2 Assets" FontSize="20" Content="&#xE960;" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="5" Width="59" Height="49" d:LayoutOverrides="TopPosition, BottomPosition" Background="#332FA614"/>
            <Button x:Name="button" FontFamily="Segoe MDL2 Assets" FontSize="20" Content="&#xE979;" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="5" Width="59" Height="49" Background="#332FA614"/>
            <Button x:Name="button2" FontFamily="Segoe MDL2 Assets" FontSize="20" Content="&#xE965;" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="5" Width="59" Height="49" Background="#332FA614"/>
        </StackPanel>
    </StackPanel>
</Page>
